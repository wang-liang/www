<span style=\"line-height:1.5;\">function ext_name3($path){</span><br />
<span style=\"line-height:1.5;\">&nbsp;&nbsp;&nbsp;&nbsp;</span><span style=\"line-height:1.5;\">$path_parts = pathinfo($path);</span><br />
