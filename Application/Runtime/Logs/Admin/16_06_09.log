&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;return $instance; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp;//保存该对象并返回<br />
